pipeline
{
    agent any

    stages
    {
        stage('Titan App Build')
        {
            steps
            {
                echo 'Build App'
            }
        }
        
        stage('Titan App Unit Tests')
        {
            steps
            {
                echo 'Run Tests'
            }
        }
        
        stage('Titan App Deploy')
        {
            steps
            {
                echo 'Deploy to artifactory'
            }
        }
    }
}
